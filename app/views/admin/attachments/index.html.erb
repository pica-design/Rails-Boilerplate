<p id="notice"><%= notice %></p>

<h1>Listing Admin Attachments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attachments.each do |admin_attachment| %>
      <tr>
        <td><%= link_to 'Show', admin_attachment %></td>
        <td><%= link_to 'Edit', edit_admin_attachment_path(admin_attachment) %></td>
        <td><%= link_to 'Destroy', admin_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attachment', new_admin_attachment_path %>
